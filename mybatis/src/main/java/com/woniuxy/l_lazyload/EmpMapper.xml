<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniuxy.l_lazyload.EmpMapper">
	<select id="findByDid" parameterType="int" resultType="Emp">
		select * from emp
		where did = #{did}
	</select>
</mapper>

